{
    "settingsSync.keybindingsPerPlatform": false,
    "telemetry.telemetryLevel": "off",
    "update.mode": "start",
    "files.autoSave": "afterDelay",
    "editor.fontFamily": "FiraCode Nerd Font Mono, Arial",
    "editor.fontSize": 12,
    "settingsSync.ignoredSettings": [
        "editor.fontSize"
    ],
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.smoothScrolling": true,
    "editor.lineNumbers": "relative",
    "editor.cursorSurroundingLines": 12,
    "editor.minimap.autohide": true,
    "editor.minimap.renderCharacters": false,
    "workbench.editor.wrapTabs": true,
    "workbench.layoutControl.enabled": false,
    "workbench.editor.pinnedTabSizing": "compact",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "vscode-neovim.useCtrlKeysForInsertMode": false,
    "workbench.colorTheme": "Panda Syntax",
    "workbench.colorCustomizations": {
        "[Panda Syntax]": {
            "editorInlayHint.background": "#00000000",
            "editorInlayHint.foreground": "#757575"
        }
    },
    "editor.semanticTokenColorCustomizations": {
        "[Panda Syntax]": {
            "enabled": true,
            "rules": {
                "namespace" : { "foreground": "#B084EB" }, 
                "class" : { "foreground": "#FFCC95" }, 
                "enum" : { "foreground": "#FFCC95"}, 
                "interface" : { "foreground": "#FFB86C", "italic": true }, 
                "struct" : { "foreground": "#FFCC95" }, 
                "typeParameter" : { "foreground": "#FFCC95", "italic": true },
                "builtinType" : { "foreground": "#FFB86C" }, 
                // "type" : { "foreground": "#FFB86C" }, 
                "parameter" : { "italic": true, "bold": true },
                // "variable" : {foregroundgro"#BCAAFE": "#FFB86C" }, 
                "variable.constant" : { "foreground": "#BCAAFE" }, 
                // "property" : { "foreground": "#FFB86Ce6e6e6, "italic": true
                "enumMember" : { "foreground": "#BCAAFE" },
                // "decorator" : { "foreground": "#FFB86C" }, 
                // "event" : { "foreground": "#FFB86C" }, 
                // "function" : { "foreground": "#FFB86C" }, 
                // "method" : { "foreground": "#FFB86C" }, 
                "macro" : { "foreground": "#6FC1FF", "underline": true, "italic": true }, 
                // "label" : { "foreground": "#B084EB", "italic": true }, 
                // "comment" : { "foreground": "#FFB86C" }, 
                "description" : { "foreground": "#ff0000" }, // NECESSARY FOR VHDL BUT NOT KNOW WHY
